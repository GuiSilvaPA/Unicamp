 lr):\n","    log = {'train_loss':[],\n","           'val_loss':[],\n","           'val_acc':[]}\n","\n","    criterion = torch.nn.CrossEntropyLoss()\n","    optimizer = torch.optim.Adagrad(model.parameters(), lr=lr, lr_decay=1e-4)\n","\n","    best_valid_acc = 0\n","    best_epoch = 0\n","\n","    for i in range(n_epochs):\n","        accumulated_loss = 0\n","        model.train()\n","        for x_train, y_train in loader_train:\n","            x_train = x_train.to(device)\n","            y_train = y_train.to(device)\n","            \n","            # predict da rede\n","            outputs = model(x_train)\n","            # print('outputs:', outputs.shape)\n","            # print('y_train:', y_train.shape)\n","            # calcula a perda\n","            batch_loss = criterion(outputs, y_train)\n","\n","            # zero, backpropagation, ajusta parâmetros pelo gradiente descendente\n","            optimizer.zero_grad()\n","            batch_loss.backward()\n","            optimizer.step()\n","            accumulated_loss += batch_loss.item()\n","            #run['train/batch_loss'].log(batch_loss)\n","\n","        train_loss = accumulated_loss / len(loader_train.dataset)\n","        log['train_loss'].append(train_loss)\n","        \n","        # Laço de Validação, um a cada época.\n","        accumulated_loss = 0\n","        accumulated_accuracy = 0\n","        model.eval()\n","        with torch.no_grad():\n","            for x_valid, y_valid in loader_valid:\n","                x_valid = x_valid.to(device)\n","                y_valid = y_valid.to(device)\n","\n","                # predict da rede\n","                outputs = model(x_valid)\n","\n","                # calcula a perda\n","                batch_loss = criterion(outputs, y_valid)\n","                preds = outputs.argmax(dim=1)\n","                # calcula a acurácia\n","                batch_accuracy = (preds == y_valid).sum()\n","                accumulated_loss += batch_loss\n","                accumulated_accuracy += batch_accuracy\n","\n","        valid_loss = accumulated_loss / len(loader_valid.dataset)\n","        valid_acc = accumulated_accuracy / len(loader_valid.dataset)\n","        log['val_loss'].append(valid_loss)\n","        log['val_acc'].append(valid_acc)\n","\n","        print(f'Epoch: {i:d}/{n_epochs - 1:d} Train Loss: {train_loss:.6f} Valid Loss: {valid_loss:.6f} -- Valid Accuracy: {valid_acc:.6f}')\n","\n","        # Salvando o melhor modelo de acordo com a loss de validação\n","        if valid_acc > best_valid_acc:\n","            torch.save(model.state_dict(), 'best_model.pt')\n","            best_valid_acc = valid_acc\n","            best_epoch = i\n","            print('best model')\n","    \n","    model.load_state_dict(torch.load('best_model.pt'))\n","    model.to(device)\n","    return model, log\n","\n","\n","def plot_loss(log):\n","    n = len(log['train_loss'])\n","    plt.plot(list(range(n)), log['train_loss'])\n","    plt.plot(list(range(n)), log['val_loss'])\n","    plt.ylabel('Loss')\n","    plt.xlabel('Epochs')\n","    plt.legend([\"Train\", \"Validation\"])\n","    plt.show()\n","\n","\n","def plot_acc(log):\n","    n = len(log['val_acc'])  \n","    plt.plot(list(range(n)), log['val_acc'])\n","    plt.ylabel('Accuracy')\n","    plt.xlabel('Epochs')\n","    plt.legend([\"Validation\"])\n","    plt.show()\n","\n","\n","def predict(model, data_loader):    \n","    predictions = []\n","    true_labels = []\n","    model.eval()\n","    with torch.no_grad():\n","        for x_valid, y_valid in data_loader:\n","            x_valid = x_valid.to(device)\n","            y_valid = y_valid.to(device)\n","\n","            # predict da rede\n","            outputs = model(x_valid)\n","            preds = outputs.argmax(dim=1)\n","            predictions.extend(preds.tolist())\n","            true_labels.extend(y_valid.tolist())\n","          \n","    acc = 100*(np.array(predictions) == np.array(true_labels)).mean()\n","    print(f'Accuracy: {acc:.2f}')\n","    return predictions, true_labels"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ezZ-dlUVi6gI","executionInfo":{"status":"ok","timestamp":1631461344549,"user_tz":180,"elapsed":256,"user":{"displayName":"Gabriel Oliveira Dos Santos","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Ghg4UvG5XTQNgOz9BG_ABG03v-LZqaqG65jgipxTw=s64","userId":"14258357965356515182"}},"outputId":"cebe4bea-052f-4c8f-8df5-21b596ff9d9b"},"source":["class NaiveClassifier(torch.nn.Module):\n","    def __init__(self, input_size, hidden_size):\n","        super(NaiveClassifier, self).__init__()\n","        self.dense = torch.nn.Sequential(\n","            torch.nn.Linear(input_size, hidden_size, bias=False),\n","            torch.nn.ReLU(),\n","            torch.nn.Linear(hidden_size, 2, bias=False)\n","        )\n","        \n","    def forward(self, x):\n","        x = self.dense(x)              \n","        return x\n","\n","if torch.cuda.is_available(): \n","   dev = \"cuda:0\"\n","   print(torch. cuda. get_device_name(dev))\n","else: \n","   dev = \"cpu\" \n","print(dev)\n","device = torch.device(dev)"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["cpu\n"]}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"KdYvHwmHi_5I","executionInfo":{"status":"ok","timestamp":1631461538503,"user_tz":180,"elapsed":3232,"user":{"displayName":"Gabriel Oliveira Dos Santos","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Ghg4UvG5XTQNgOz9BG_ABG03v-LZqaqG65jgipxTw=s64","userId":"14258357965356515182"}},"outputId":"f5924f11-79e6-4c91-adea-144896b358e1"},"source":["reset_seeds()\n","\n","train_dataset = TensorDataset(torch.FloatTensor(X_train_bow_count),\n","                              torch.LongTensor(y_train))\n","bow_count_train_data_loader = DataLoader(train_dataset, \n","                              batch_size=100,\n","  