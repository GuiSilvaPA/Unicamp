rce":["import re\n","import collections\n","\n","class FeatureExtractor:\n","  def __init__(self, vocab, padding_token='[PAD]', unk_token='[UNK]'):\n","    self.vocab = [padding_token, unk_token] + vocab\n","    self.unk_token = unk_token\n","    self.padding_token = padding_token\n","    self.word_to_id = {word:id for id, word in enumerate(self.vocab)}\n","    self.vocab_size = len(self.word_to_id)\n","\n","\n","  def bag_of_words_count(self, list_tokenized_sents, ignore_pad=True):\n","    n = len(list_tokenized_sents)    \n","    features = np.zeros((n, self.vocab_size))    \n","    for i, tokens in enumerate(list_tokenized_sents):\n","      for w in tokens:\n","        if not ignore_pad or (ignore_pad and w !=  self.padding_token):\n","          id = self.word_to_id.get(w, self.word_to_id[self.unk_token])          \n","          features[i, id] += 1\n","    if ignore_pad:\n","      return np.delete(features, self.word_to_id[self.padding_token], 1)\n","\n","    return features\n","\n","\n","  def get_word_idx(self, list_tokenized_sents):\n","    output = []\n","    for tokens in list_tokenized_sents:\n","      word_idx = []\n","      for w in tokens:        \n","          id = self.word_to_id.get(w, self.word_to_id[self.unk_token])\n","          word_idx.append(id)\n","      output.append(word_idx)\n","    return output"],"execution_count":4,"outputs":[]},{"cell_type":"code","metadata":{"id":"NKlohD2rWH40","executionInfo":{"status":"ok","timestamp":1631729596884,"user_tz":180,"elapsed":5,"user":{"displayName":"Guilherme Pereira Albino da Silva","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"07971392613373651702"}}},"source":["def get_vocab(processed_sents_train, vocab_size, padding_token='[PAD]'):    \n","    raw_vocab = []\n","    for words in processed_sents_train:\n","      raw_vocab.extend([w for w in words if w != padding_token])\n","\n","    freq = collections.Counter(raw_vocab)\n","    padding_token\n","    most_common = freq.most_common(vocab_size) \n","    return [word for word, _ in most_common]\n","\n","def tokenize(text):\n","  tokenizer = re.compile(r'(\\w+\\'?\\w*|[?!.,:;\"])')\n","  return tokenizer.findall(text)       \n","\n","def preprocess_sentence(sent, padding_token='[PAD]', max_length=200):\n","  tokenized_sent = tokenize(sent)\n","  if len(tokenized_sent) > max_length:\n","    return tokenized_sent[:max_length]\n","  \n","  diff_len = max_length - len(tokenized_sent)\n","  return tokenized_sent + [padding_token]*diff_len"],"execution_count":5,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"P9AVASHZhYaS"},"source":["Testando o método de pré-processamento"]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1oOKbKPAedXX","executionInfo":{"status":"ok","timestamp":1631729599636,"user_tz":180,"elapsed":236,"user":{"displayName":"Guilherme Pereira Albino da Silva","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"07971392613373651702"}},"outputId":"e2ef3d95-5d30-45d8-9320-8f723e69adcb"},"source":["texts = ['i like pepperoni pizza', 'i enjoy going to the beach']\n","preprocessed_texts = [preprocess_sentence(text, max_length=5) for text in texts]\n","vocab = get_vocab(preprocessed_texts, vocab_size=5)  \n","print(vocab)"],"execution_count":6,"outputs":[{"output_type":"stream","name":"stdout","text":["['i', 'like', 'pepperoni', 'pizza', 'enjoy']\n"]}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"tPyveBbigZeq","executionInfo":{"status":"ok","timestamp":1631729601526,"user_tz":180,"elapsed":316,"user":{"displayName":"Guilherme Pereira Albino da Silva","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"07971392613373651702"}},"outputId":"203f1ac7-3e81-4202-808e-45ebc8514d7d"},"source":["extractor = FeatureExtractor(vocab)\n","\n","print('vocab:', vocab)\n","print('word_to_id:', extractor.word_to_id)"],"execution_count":7,"outputs":[{"output_type":"stre